{
	"name": "vscode-xml-maven",
	"displayName": "vscode-xml-maven",
	"publisher": "vscmaven",
	"description": "Maven support with XML Language Server",
	"version": "0.0.1",
	"repository": {
		"type": "git",
		"url": "https://github.com/angelozerr/vscode-xml-maven.git"
	},
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"xml.javaExtensions": [
			"./jar/org.eclipse.lsp4xml.extensions.maven-0.9.1.jar"
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.1",
		"@types/node": "^14.14.28",
		"@types/vscode": "^1.5.1",
		"glob": "^7.1.3",
		"mocha": "^8.3.0",
		"typescript": "^4.1.5",
		"tslint": "^6.1.2",
		"vscode-test": "^1.5.1",
		"vsce": "^1.85.0"
	}
}